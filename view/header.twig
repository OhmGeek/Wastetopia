<!DOCTYPE html>
<html>
	<head>
	  <meta charset="utf-8">
	  <title>{% block title %}{% endblock %}</title>
	  <!-- Material Design fonts -->
	  <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700" type="text/css">
	  <link href="//fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

	  <!-- Bootstrap -->
	  <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
	  <!-- jQuery library -->
	  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>

	  <!-- Latest compiled JavaScript -->
	  <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
	  <!-- Bootstrap Material Design -->
	  <link href="{{config.ROOT_BASE}}/bootstrap-material-design-master/dist/css/bootstrap-material-design.css" rel="stylesheet">
	  <link href="{{config.ROOT_BASE}}/bootstrap-material-design-master/dist/css/ripples.min.css" rel="stylesheet">
	  <link href="http://fezvrasta.github.io/snackbarjs/dist/snackbar.min.css" rel="stylesheet">

	  <!-- Wastopia Custom CSS -->
	  <link rel="stylesheet" href="{{config.ROOT_CSS}}/wastetopia-main.css"/>

	  <meta name="viewport" content="width=device-width, initial-scale=1.0">
	</head>
	<body>


        {% block navbar %}
	{% endblock %}
		
	{% block content %}
	{% endblock %}



        <script src="{{config.ROOT_BASE}}/bootstrap-material-design-master/dist/js/ripples.min.js"></script>
        <script src="{{config.ROOT_BASE}}/bootstrap-material-design-master/dist/js/material.min.js"></script>

        <!-- script for having equal height rows -->
        <script src="{{config.ROOT_BASE}}/plugins/jquery-match-height/jquery.matchHeight.js" type="text/javascript"></script>

        <script>
          $(function () {
            $.material.init();
          });
        </script>
        <!-- Wastopia Custom JS-->
        {% for jsfile in jslist %}
            <script src="{{config.ROOT_JS}}/{{jsfile}}"></script>
        {% endfor %}
	</body>
</html>
